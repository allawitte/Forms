.form-part .panel-heading {
    padding: 6px 15px;
}
.left-part .panel {
    border: none;
    box-shadow: node;
    -webkit-box-shadow: none;
}
.panel .bootstrap-select:not([class*="col-"]):not([class*="form-control"]):not(.input-group-btn) {
    width: 120px;
    float: right;
}
.form-selects.form-inline
{
    height: 319px;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 14px;
}
.form-selects.form-inline .form-group, .form-data.form-inline .form-group{
    width: 100%;
    padding: 5px 0;
}
.bootstrap-select .btn {
    padding: 2px 12px;
}
.form-selects .form-control {
    float: right;
    padding: 2px 12px;
    width: 120px;
    height: 28px;
}
.form-data .form-control {
    float: right;
    padding: 2px 12px;
    width: 70%;
    height: 28px;
}
.form-inline .form-group label
{
    color: #0b6c9d;
}
.form-serie .form-part {
    padding-left: 3px;
    padding-right: 3px;
}
.form-serie .panel-heading {
    text-align: center;
    background-color: #ff0000;
    color: #ffddb0;
    background-image: none;
    font-weight: bold;
    font-size: 18px;
}
.form-serie .panel-footer {
    text-align: center;
}
.form-serie .btn-default {
    color: #0b6c9d;
    border: 1px solid #0b6c9d;
    background-color: transparent;
}
.form-serie .btn-lg {
    display: block;
    background-color: #00d100;
    color: #fff;
    margin: 10px auto 0 auto;
    padding: 10px 40px;
}
.active > .dropdown-menu {
    display: block;
}
.dropdown-menu .inner {
    max-height: 304px; 
    overflow-y: auto; 
    min-height: 80px;
    }